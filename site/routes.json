{
  "/": {
    "layout": "siteIndex",
    "meta": {
      "title": "SurviveJS",
      "description": "Get started with JavaScript and related technologies"
    },
    "dataSources": {
      "blogPosts": {
        "operation": "indexBlog",
        "parameters": ["./pages/blog", 8]
      }
    }
  },
  "blog": {
    "layout": "blogIndex",
    "meta": {
      "title": "Blog",
      "description": "Read blog interviews to learn more about JavaScript and related technologies"
    },
    "dataSources": {
      "blogPosts": {
        "operation": "indexBlog",
        "parameters": ["./pages/blog"]
      }
    },
    "expand": {
      "matchBy": {
        "indexer": {
          "operation": "indexBlog",
          "parameters": ["./pages/blog"]
        },
        "slug": "data.slug"
      },
      "layout": "documentationPage",
      "dataSources": {
        "document": {
          "operation": "processBlogPost",
          "parameters": [
            { "parseHeadmatter": true }
          ]
        }
      },
      "meta": {
        "title": {
          "utility": "get",
          "parameters": ["context", "document.data.title"]
        },
        "description": {
          "utility": "get",
          "parameters": ["context", "document.data.description"]
        }
      }
    },
    "routes": {
      "topics": {
        "layout": "topicIndex",
        "meta": {
          "title": "Topics",
          "description": "Blog topics"
        },
        "dataSources": {
          "topics": {
            "operation": "indexTopics",
            "parameters": ["./pages/blog"]
          }
        },
        "expand": {
          "matchBy": {
            "indexer": {
              "operation": "indexTopics",
              "parameters": ["./pages/blog"]
            },
            "slug": "slug"
          },
          "layout": "topicPage",
          "dataSources": {
            "topic": {
              "operation": "processTopic"
            }
          },
          "meta": {}
        }
      }
    }
  },
