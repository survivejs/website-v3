{
  "/": {
    "layout": "siteIndex",
    "meta": {
      "title": "SurviveJS",
      "description": "Get started with JavaScript for free"
    },
    "dataSources": [
      {
        "operation": "processMarkdown",
        "parameters": [{ "path": "./README.md" }, { "skipFirstLine": true }],
        "name": "readme"
      }
    ],
    "scripts": [{ "name": "hello" }]
  },
  "blog": {
    "layout": "blogIndex",
    "meta": {
      "title": "Blog",
      "description": "Read blog interviews to learn more about JavaScript and related technologies"
    },
    "dataSources": [
      {
        "operation": "indexBlog",
        "parameters": ["./pages/blog"],
        "name": "blogPosts"
      }
    ],
    "expand": {
      "matchBy": {
        "indexer": {
          "operation": "indexBlog",
          "parameters": ["./pages/blog"]
        },
        "dataSources": [{
          "operation": "processBlogPost",
          "parameters": [
            { "parseHeadmatter": true }
          ],
          "name": "document"
        }],
        "slug": "data.slug"
      },
      "layout": "documentationPage",
      "meta": {
        "title": {
          "utility": "get",
          "parameters": ["context", "document.data.title"]
        },
        "description": {
          "utility": "get",
          "parameters": ["context", "document.data.description"]
        }
      }
    },
    "routes": {
      "topics": {
        "layout": "topicIndex",
        "meta": {
          "title": "Topics",
          "description": "Blog topics"
        },
        "dataSources": [
          {
            "operation": "indexTopics",
            "parameters": ["./pages/blog"],
            "name": "topics"
          }
        ],
        "expand": {
          "matchBy": {
            "indexer": {
              "operation": "indexTopics",
              "parameters": ["./pages/blog"]
            },
            "dataSources": [{
              "operation": "processTopic",
              "name": "topic"
            }],
            "slug": "slug"
          },
          "layout": "topicPage",
          "meta": {}
        }
      }
    }
  },
